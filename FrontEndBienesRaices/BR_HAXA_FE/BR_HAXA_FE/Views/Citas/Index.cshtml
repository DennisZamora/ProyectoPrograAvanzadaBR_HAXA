            <th>
                @Html.DisplayNameFor(model => model.IdUsuarioNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaFinal)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.IdPropiedadNavigation.Canton)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuarioNavigation.Apellido1)

@model IEnumerable<BR_HAXA_FE.WIZARD.Models.Imagenes>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Link)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPropiedadNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Link)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPropiedadNavigation.Canton)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdImagen">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdImagen">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdImagen">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
